<div id="users">
<div id="wrap">
  <div id="header" align="center" style="margin-top: 30px">
    <img src="assets/images/logo.png">
  </div>
  <div id="content">
    <div class="form-wrapper">
      <h1>Sign Up</h1>
      <%= form_for(@user) do |f| %>
        <div class="form-item">
          <label for="username"></label>
          <%= f.text_field :name, id:"username", placeholder:"UserName" %>
        </div>
        <div class="form-item">
          <label for="email"></label>
          <%= f.password_field :email, id:"email", placeholder:"Email Address" %>
        </div>
        <div class="button-panel">
          <%= f.submit "Sign Up", class: "btn btn-primary", value:"Sign Up" %>
        </div>
      <% end %>
      <br>
      <br>
    </div>
  </div>
</div>
